<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="antiguedadsaldos" language="groovy" pageWidth="792" pageHeight="612" orientation="Landscape" columnWidth="752" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<parameter name="relativepath" class="java.lang.String"/>
	<parameter name="pcondicion" class="java.lang.String"/>
	<parameter name="pfecha" class="java.lang.String"/>
	<queryString>
		<![CDATA[select "Antiguedad de saldos" as nombrerep, concat("Fecha de Corte ", $P!{pfecha}) as periodo,almacen,idcliente,cliente,idfactura,
moneda,fechafactura,fechavencimiento,sum(saldo) as saldo,sum(saldop) as saldop,sum(columna1)+sum(columna2)+sum(columna3)+sum(columna4) saldoven,
sum(columna1p)+sum(columna2p)+sum(columna3p)+sum(columna4p) saldovenp,sum(xvencer) as xvencer,sum(xvencerp) as xvencerp,
sum(columna1) as columna1,sum(columna1p) as columna1p,sum(columna2) as columna2,sum(columna2p) as columna2p,
sum(columna3) as columna3,sum(columna3p) as columna3p, sum(columna4) as columna4,sum(columna4p) as columna4p,procedencia
FROM(
select almacen,idcliente,cliente,idfactura,moneda,fechafactura,fechavencimiento,
round(saldo,2) as saldo,round(if(idmoneda=2,saldo*tc,saldo),2) as saldop,
round(if(dias<0,saldo,0),2) as xvencer,round(if(dias<0,if(idmoneda=2,saldo*tc,saldo),0),2) as xvencerp,
round(if(dias>=0 and dias<31,saldo,0),2) as columna1,round(if(dias>=0 and dias<31,if(idmoneda=2,saldo*tc,saldo),0),2) as columna1p,
round(if(dias>30 and dias<61,saldo,0),2) as columna2,round(if(dias>30 and dias<61,if(idmoneda=2,saldo*tc,saldo),0),2) as columna2p,
round(if(dias>60 and dias<91,saldo,0),2) as columna3,round(if(dias>60 and dias<91,if(idmoneda=2,saldo*tc,saldo),0),2) as columna3p,
round(if(dias>90,saldo,0),2) as columna4, round(if(dias>90,if(idmoneda=2,saldo*tc,saldo),0),2) as columna4p, procedencia
FROM
(
select a.nombrecorto as almacen,f.idcliente, idfactura,f.idmoneda,m.moneda,f.tc,
(select saldo from ccpagosclientes pc where pc.iddocumento=f.idfactura and fechacreacion<=$P!{pfecha} order by idcontador desc limit 1) as saldo,
fechafactura,fechavencimiento,
if(c.persona ="F",if(c.apaterno is NULL or c.apaterno="",c.nombre,
if(c.amaterno is NULL or c.amaterno="",concat(c.nombre, " ",c.apaterno),concat(c.nombre, " ",c.apaterno," ", c.amaterno))), c.rsocial) as cliente,
DATEDIFF(curdate(),fechavencimiento) as dias,if(procedencia='R','Refacciones',if(procedencia='S','Servicio','Unidades Nuevas')) as procedencia
from facturas f
left join clientes c on c.idcliente=f.idcliente
left join realmacen a on a.idalmacen=f.idalmacen
left join monedas m on m.idmoneda=f.idmoneda
where saldado=0 and f.fechafactura<=$P!{pfecha}  $P!{pcondicion}
UNION
select a.nombrecorto as almacen,ngo.idcliente, concat(ngo.serie,ngo.idnotacgo) as docto, ngo.idmoneda,m.moneda,ngo.tc,
(select saldo from ccpagosclientes pc where pc.iddocumento=concat(ngo.serie,ngo.idnotacgo) and fechacreacion<=$P!{pfecha} order by idcontador desc limit 1) as saldo,ngo.fechacreacion,fechavencimiento,
if(c.persona ="F",if(c.apaterno is NULL or c.apaterno="",c.nombre,
if(c.amaterno is NULL or c.amaterno="",concat(c.nombre, " ",c.apaterno),concat(c.nombre, " ",c.apaterno," ", c.amaterno))), c.rsocial) as cliente,
DATEDIFF(curdate(),fechavencimiento) as dias,if(procedencia='R','Refacciones',if(procedencia='S','Servicio','Unidades Nuevas')) as procedencia
from notascargo ngo
left join clientes c on c.idcliente=ngo.idcliente
left join realmacen a on a.idalmacen=ngo.idalmacen
left join monedas m on m.idmoneda=ngo.idmoneda
where saldado=0 and ngo.fechacreacion<=$P!{pfecha}  $P!{pcondicion}
) as t
) as t GROUP BY almacen,moneda,procedencia,idcliente]]>
