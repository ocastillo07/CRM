	<queryString>
		<![CDATA[select $P{pnombrerep} as nombrerep,$P{pperiodo} as periodo,
al.nombrecorto as almacen,m.moneda,concat(nc.serie,nc.idnotacgo) as notacargo,"" as procedencia,"" as notacredito,
nc.idcliente,if(c.persona ="F",if(c.apaterno is NULL or c.apaterno="",c.nombre,if(c.amaterno is NULL or c.amaterno="",concat(c.nombre, " ",c.apaterno),
concat(c.nombre, " ",c.apaterno," ", c.amaterno))), c.rsocial) as cliente,nc.fechacreacion,
subtotal,iva,round(total,2) as total,round(if(nc.idmoneda=1,total,0),2) as totalmn,round(if(nc.idmoneda=2,total,0),2) as totaldlls,nce.iniciales as estatus,nt.nombre as tipo
from notascargo nc
left join clientes c on c.idcliente=nc.idcliente
left join realmacen al on al.idalmacen=nc.idalmacen
left join monedas m on m.idmoneda=nc.idmoneda
left join notascargoestatus nce on nce.idestatus=nc.idestatus
left join notascargotipos nt on nt.idtipo=nc.idtipo
$P!{pcondicion}
order by almacen,moneda,notacargo]]>
