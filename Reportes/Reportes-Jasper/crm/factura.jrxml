	<queryString>
		<![CDATA[select v.idfactura, v.idcliente, if(c.rsocial is null or c.rsocial='', concat(c.nombre,' ',c.apaterno,' ',c.amaterno),c.rsocial) as cliente,c.rfc,c.cp,c.direccion,c.colonia,c.municipio, c.estado,c.telefono,concat(u.nombre,' ',u.apaterno,' ',u.amaterno) as promotor,v.fechacreacion,tp.nombre as tipocobro,
round(sum(d.cantidad*d.precio),2) as subtotal,round(iva,2) as iva,round(total,2) as total,
m.iniciales as moneda
from reventas v
left join reventasdet d on d.idventa=v.idventa and d.idrevision=v.idrevision and d.serie=v.serie
left join clientes c on c.idcliente=v.idcliente
left join usuarios u on u.idusuario= v.idpromotor
left join retipopagos tp on tp.idtipopago=v.idtipopago
left join monedas m on m.idmoneda=v.idmoneda
where v.idventa=$P{idventa} and v.idrevision=$P{idrevision} and v.serie=$P!{serie}]]>
	</queryString>
